{
  "name": "angular-clean-architecture",
  "version": "1.0.0",
  "description": "Angular Clean Architecture Boilerplate with i18n, TypeScript and best practices",
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "keywords": [
    "angular",
    "clean-architecture",
    "typescript",
    "i18n",
    "boilerplate",
    "primeng",
    "rxjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/angular-clean-architecture.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/angular-clean-architecture/issues"
  },
  "homepage": "https://github.com/your-username/angular-clean-architecture#readme",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=dev",
    "start:prod": "ng serve --configuration=production",
    "start:qa": "ng serve --configuration=qa",
    "start:dev": "ng serve --configuration=dev",
    "start:fake": "ng serve --configuration=fake",
    "build": "ng build --configuration=production",
    "build:prod": "ng build --configuration=production",
    "build:qa": "ng build --configuration=qa",
    "build:dev": "ng build --configuration=dev",
    "watch": "ng build --watch --configuration=dev",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --watchAll=false",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write --ignore-unknown ./src",
    "format:check": "prettier --check --ignore-unknown ./src",
    "type-check": "tsc --noEmit",
    "generate:feature": "ng generate ./schematics/feature:feature",
    "i18n:extract": "ng extract-i18n",
    "i18n:build": "ng build --localize",
    "build:schematics": "tsc -p schematics/tsconfig.json",
    "pre-commit": "npm run format && npm run type-check",
    "postinstall": "npm run build:schematics"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/common": "^17.3.12",
    "@angular/compiler": "^17.3.12",
    "@angular/core": "^17.3.12",
    "@angular/forms": "^17.3.12",
    "@angular/platform-browser": "^17.3.12",
    "@angular/platform-browser-dynamic": "^17.3.12",
    "@angular/router": "^17.3.12",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.18.11",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular-eslint/builder": "17.5.3",
    "@angular-eslint/eslint-plugin": "17.5.3",
    "@angular-eslint/eslint-plugin-template": "17.5.3",
    "@angular-eslint/schematics": "17.5.3",
    "@angular-eslint/template-parser": "17.5.3",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.4.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  }
}